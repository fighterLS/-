OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/AFNetworking" -isystem "${PODS_ROOT}/Headers/Public/APParallaxHeader" -isystem "${PODS_ROOT}/Headers/Public/IQKeyboardManager" -isystem "${PODS_ROOT}/Headers/Public/MBProgressHUD" -isystem "${PODS_ROOT}/Headers/Public/Masonry" -isystem "${PODS_ROOT}/Headers/Public/UIImage+BlurredFrame" -isystem "${PODS_ROOT}/Headers/Public/YYKit" -isystem "${PODS_ROOT}/Headers/Public/YYKit/WebP" -isystem "${PODS_ROOT}/Headers/Public/pop"
